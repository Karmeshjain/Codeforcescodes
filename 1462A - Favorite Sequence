#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
    	int n;cin>>n;int j=0;
         long long a[n],b[n],flag=0;
         for(int i=0;i<n;i++)
         cin>>a[i];
        for(int i=0;i<(n+1)/2;i++){
        b[2*i]=a[i];
        b[2*i+1]=a[n-i-1];
        }
         for(int i=0;i<n;i++){
       cout<<b[i]<<" ";
        }
        cout<<endl;
    }
   return 0;
}
