#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
    	string s;
    	cin>>s;
    	if(s.length()==2)
    	cout<<s;
    	else
        {
		for(int i=0;i<s.length();i=i+2)
    	cout<<s[i];
    	cout<<s[s.length()-1];
        }
    	cout<<endl;
	}
    return 0;
}
