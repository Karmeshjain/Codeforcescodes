#include <bits/stdc++.h> 
using namespace std; 
int main()
{	
    int t;
    cin>>t;
    while(t--)
    {
    int n,c1=0,c2=0;
    cin>>n;int k;
    int sum=0;
    for(int i=0;i<n;i++)
    	{
		cin>>k;
		k==1?c1++:c2++;
		}
		((c1%2)||(c2%2&&c1<2))?cout<<"NO":cout<<"YES";
		cout<<endl;
    }
	return 0;
}
