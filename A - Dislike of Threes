#include <bits/stdc++.h> //KarmeshDuggar
using namespace std;
#define ll long long

int main() 
{
	
    cin.tie(NULL);
    ios::sync_with_stdio(false);
    vector<int> res;
	int j=0;
	unordered_map<int,int> mp;
    for(int i=1;i<=1666;i++)
    {
    	if(i%3==0 || i%10==3)
	    continue;
	    else
		mp[j++]=i;
	}
    int t;
    cin>>t;
    while(t--)
    {
     int k;
     cin>>k;
     cout<<mp[k-1]<<endl;
	}
    return 0;
}
